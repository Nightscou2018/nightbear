{
  "name": "nightbear-api",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run js-watch & sleep 5 && npm run serve-dev",
    "js-build": "tsc --project .",
    "js-watch": "tsc --project . --watch --pretty",
    "serve-dev": "source dev.env && nodemon --watch . express.js",
    "serve-prod": "source prod.env && nodemon --watch . express.js",
    "deploy-dev": "source dev.env && serverless deploy --stage dev",
    "deploy-prod": "source prod.env && serverless deploy --stage prod"
  },
  "license": "AGPL",
  "dependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.33",
    "@types/lodash": "^4.14.36",
    "@types/node": "^6.0.41",
    "@types/winston": "0.0.28",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "lodash": "^4.16.3",
    "nodemon": "^1.10.2",
    "serverless": "^1.0.0-rc.2",
    "serverless-plugin-write-env-vars": "^1.0.1",
    "typescript": "^2.0.3",
    "winston": "^2.2.0",
    "winston-papertrail": "^1.0.3"
  }
}
