{
  "logLevel": "debug",
  "proxyTimeout": 5000,
  "rewriteConfig": {
    "^/api/v1/(.*)": [
      "https://server-stage.nightbear.fi/$1"
    ],
    "^/parakeet(.*)": [
      "https://server-stage.nightbear.fi/upload-parakeet-entry$1"
    ]
  },
  "proxiedIncomingHeaders": [
    "authorization",
    "content-type",
    "user-agent",
    "x-request-id"
  ],
  "proxiedOutgoingHeaders": [
    "content-type",
    "cache-control"
  ]
}
