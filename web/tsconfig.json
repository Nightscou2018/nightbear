{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "allowJs": false,
    "declaration": false,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": ["es6", "es7", "dom"],
    "baseUrl": ".",
    "paths": {
      "app/*": ["./app/*"]
    }
  },
  "exclude": ["dist", "build", "node_modules"]
}
